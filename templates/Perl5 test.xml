<templateSet group="Perl5 test">
  <template name="subtest" value="subtest $NAME$ =&gt; sub {&#10;    $END$&#10;};" description="Subtest" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="testname" alwaysStopAt="true" />
    <context />
  </template>
  <template name="ok" value="ok $COND$;&#10;$END$" description="ok assertion" toReformat="false" toShortenFQNames="true">
    <variable name="COND" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="is" value="is $COND$, $EXPECTED$;&#10;$END$" description="is assertion" toReformat="false" toShortenFQNames="true">
    <variable name="COND" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="EXPECTED" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="is_deeply" value="is_deeply $GOT$, $EXPECTED$;&#10;$END$" description="is_deepy assertion" toReformat="false" toShortenFQNames="true">
    <variable name="GOT" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="EXPECTED" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="note" value="note $TEST_NAME$;&#10;$END$" description="note statement" toReformat="false" toShortenFQNames="true">
    <variable name="TEST_NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
</templateSet>